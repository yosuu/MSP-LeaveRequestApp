                                </li>
                                <li id="menuStatus">
                                    <a href="/master/requeststatus" class="nav-link"> Request Status </a>
                                </li>
                                <li id="menuDepartment">
                                    <a href="/master/department" class="nav-link"> Department </a>
                                </li>
                                <li>
                                    <a href="/master/roles" class="nav-link"> Roles </a>
                                </li>
                                <li>
                                    <a href="/master/employee" class="nav-link"> Employee </a>
                                </li>
                            </ul>
                        </li>*@
                    </ul>
                </div>
                <!-- END MEGA MENU -->
            </div>
        </div>
        <!-- END HEADER MENU -->
    </div>
    <!-- END HEADER -->
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN CONTENT -->
        @RenderBody()
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <!-- BEGIN INNER FOOTER -->
    <div class="page-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    @DateTime.Now.Year &copy; Leave Request App by Yoshua Andrew.
                    <ul class="social-icons" style="margin-top: 5px;">
                        <li>
                            <a href="https://facebook.com/andrew.houtama" target="_blank" data-original-title="facebook" class="facebook"></a>
                        </li>
                        <li>
                            <a href="https://twitter.com/yoshua_andrew" target="_blank" data-original-title="twitter" class="twitter"></a>
                        </li>
                        <li>
                            <a href="https://www.instagram.com/yoshuaandrew" target="_blank" data-original-title="instagram" class="instagram"></a>
                        </li>
                        <li>

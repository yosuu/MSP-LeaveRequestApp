                <!-- DOC: Remove data-hover="dropdown" and data-close-others="true" attributes below to disable the dropdown opening on mouse hover -->
                <div class="hor-menu">
                    <ul class="nav navbar-nav">
                        <li id="menuDashboard">
                            <a href="/">
                                Dashboard
                            </a>
                        </li>
                        <li id="menuHistory">
                            <a href="/history">
                                History
                            </a>
                        </li>
                        <li id="menuRequest">
                            <a href="/request">
                                New Request
                            </a>
                        </li>
                        @*<li id="menuMaster" class="menu-dropdown classic-menu-dropdown ">
                            <a href="javascript:;">
                                Master Data
